{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "align": [
            true,
            "elements",
            "members",
            "statements"
        ],
        "arrow-parens": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}